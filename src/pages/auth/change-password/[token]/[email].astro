---
import AppContainer from "../../../../layouts/AppContainer.astro";
import ChangePass from "../../../../modules/user/aplication/ForgetPassword/ChangePass.astro";

const {token,email} = Astro.params

if(Astro.cookies.get("eons_lng")?.value === 'es'){
    return Astro.redirect(`/es/auth/change-password/${token}/${email}`)
 }
---

<AppContainer>
    <ChangePass token={token} email={email}/>
</AppContainer>
